    @AuraEnabled(Cacheable=true)
    Public Static List<Decimal> QueryAllMetadataSeries(){
        
       List<ProductMetadata__mdt> TempMetdata=[Select Field_Api_Name__c,Field_Label__c,Field_Type__c,Required__c,Series_Number__c From ProductMetadata__mdt];
        system.debug('TempMetdata,'+TempMetdata);
        set<Decimal> Allseries=new set<Decimal>();
        
        for(ProductMetadata__mdt Tempdata:TempMetdata){
            Allseries.add(Tempdata.Series_Number__c); 
        }
        List<Decimal> seriesList=new List<Decimal>(Allseries);
        
        
        system.debug('Allseries--'+Allseries);
        system.debug('seriesList--'+seriesList);
        
        return seriesList;
    }
    
    @AuraEnabled(Cacheable=true)
    Public static Map<Decimal,List<ProductMetadata__mdt>> QueryAllMetadata(){
        
        List<ProductMetadata__mdt> TempMetdata=[Select Field_Api_Name__c,Field_Label__c,Field_Type__c,Required__c,Series_Number__c From ProductMetadata__mdt];
        system.debug('TempMetdata,'+TempMetdata);
        
        Map<Decimal,List<ProductMetadata__mdt>> seriesXMetadata=new Map<Decimal,List<ProductMetadata__mdt>>();

        for(ProductMetadata__mdt Tempdata:TempMetdata){
            
            if(seriesXMetadata.containskey(Tempdata.Series_Number__c)){
                List<ProductMetadata__mdt> tempmeta=seriesXMetadata.get(Tempdata.Series_Number__c);
                tempmeta.add(Tempdata);
                seriesXMetadata.put(Tempdata.Series_Number__c,tempmeta);
            }else{
                List<ProductMetadata__mdt> tempmetavalue=new List<ProductMetadata__mdt>();
                tempmetavalue.add(Tempdata);
                seriesXMetadata.put(Tempdata.Series_Number__c,tempmetavalue);
            }
        }
        system.debug('seriesXMetadata--'+seriesXMetadata);
       return  seriesXMetadata;
    }
    
